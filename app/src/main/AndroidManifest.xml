<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.northlandcaps.crisis_response">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/NormalTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <service android:name=".FcmInstanceIDService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT"></action>
            </intent-filter>
        </service>
        <service android:name=".FcmMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT"></action>
            </intent-filter>
        </service>
        <activity android:name=".AdminMenu" />
        <activity android:name=".usermanagement" />
        <activity android:name=".location_log_activity" />
        <activity android:name=".buildingmangement" />
        <activity android:name=".CreateUser" />
        <activity android:name=".roommanagement" />
        <activity android:name=".setingshandler" />
        <activity android:name=".CreateBuilding" />
        <activity android:name=".EditBuilding"/>
        <activity android:name=".CreateRoom"></activity>
    </application>

</manifest>